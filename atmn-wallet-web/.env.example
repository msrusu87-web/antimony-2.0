# Web Wallet Configuration

## Web Server
REACT_APP_API_URL=https://rpc.antimony.carphatian.ro
REACT_APP_EXPLORER_URL=https://explorer.carphatian.ro
REACT_APP_CHAIN_ID=7676

## Wallet Features
REACT_APP_HARDWARE_WALLET=true
REACT_APP_2FA_ENABLED=true
REACT_APP_MULTI_SIG=true
REACT_APP_STAKING=true

## Token Standards
REACT_APP_ERC20_SUPPORT=true
REACT_APP_ERC721_SUPPORT=true
REACT_APP_ERC1155_SUPPORT=true
REACT_APP_SWAP_ENABLED=true

## Swap Providers
REACT_APP_DEX_UNISWAP=true
REACT_APP_DEX_PANCAKESWAP=true
REACT_APP_DEX_RAYDIUM=true

## Security
REACT_APP_ENCRYPTION=AES256
REACT_APP_KEYSTORE_VERSION=4
REACT_APP_SESSION_TIMEOUT=1800  # seconds
REACT_APP_PIN_LENGTH=6

## MetaMask Integration
REACT_APP_METAMASK_RPC=https://rpc.antimony.carphatian.ro
REACT_APP_METAMASK_CHAIN_ID=7676

## WalletConnect
REACT_APP_WALLETCONNECT_PROJECT_ID=your_project_id_here

## Logging
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info

## Analytics
REACT_APP_MIXPANEL_TOKEN=your_token
REACT_APP_SENTRY_DSN=your_sentry_dsn
